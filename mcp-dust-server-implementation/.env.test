# Test environment configuration

# Server configuration
NODE_ENV=test
MCP_SERVER_NAME=MCP Dust Server Test
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=5002
MCP_REQUEST_TIMEOUT=5

# Dust API configuration
DUST_API_KEY=test-api-key
DUST_WORKSPACE_ID=test-workspace-id
DUST_AGENT_ID=test-agent-id
DUST_USERNAME=test-user
DUST_EMAIL=test@example.com
DUST_FULL_NAME=Test User
DUST_TIMEZONE=UTC

# CORS configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000

# Security configuration
SECURITY_SECRET_KEY=test-secret-key
SECURITY_TOKEN_EXPIRATION=3600
SESSION_TTL=3600000

# Logging configuration
LOG_LEVEL=error
LOG_FORMAT=pretty
LOG_REQUEST_BODY=false
LOG_REQUEST_HEADERS=false
LOG_RESPONSE_BODY=false
LOG_RESPONSE_HEADERS=false
