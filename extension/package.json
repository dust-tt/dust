{
  "name": "@dust-tt/extension",
  "version": "0.0.1",
  "description": "Dust Chrome Extension",
  "scripts": {
    "clean": "rm -rf build/*",
    "dev:chrome": "npm run clean && tsx run/watch.ts --platform=chrome --mode=development",
    "dev:front": "npm run clean && tsx run/watch.ts --platform=front --mode=development",
    "package:chrome:production": "tsx run/build.ts --platform=chrome",
    "package:front:production": "tsx run/build.ts --platform=front",
    "package:chrome:release": "tsx run/build.ts --platform=chrome --release",
    "analyze:chrome": "tsx run/build.ts --platform=chrome --analyze",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "tsgo": "tsgo"
  },
  "author": "@dust-tt",
  "license": "ISC",
  "devDependencies": {
    "@types/chrome": "^0.0.276",
    "@types/dotenv-webpack": "^7.0.8",
    "@types/event-source-polyfill": "^1.0.5",
