  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Debug front server",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env.local"
    },
    {
      "type": "node-terminal",
      "name": "Debug front worker",
      "request": "launch",
      "command": "npm run start:worker",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env.local"
    }
  ]
}
