  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "description": "The type of activity related to the pull request",
      "enum": [
        "assigned",
        "auto_merge_disabled",
        "auto_merge_enabled",
        "closed",
        "converted_to_draft",
        "dequeued",
        "edited",
        "enqueued",
        "labeled",
        "locked",
        "merged",
        "opened",
        "ready_for_review",
        "reopened",
        "review_request_removed",
        "review_requested",
        "synchronize",
        "unassigned",
        "unlabeled",
        "unlocked"
      ]
    },
    "number": {
      "type": "integer",
      "description": "The pull request number"
    },
    "changes": {
      "type": "object",
      "description": "The changes to the pull request if the action was edited",
      "properties": {
        "title": {
          "type": "object",
          "description": "Changes to the pull request title",
          "properties": {
            "from": {
              "type": "string",
              "description": "The previous version of the title if the action was edited"
            }
          }
        },
        "body": {
          "type": "object",
          "description": "Changes to the pull request body",
          "properties": {
            "from": {
              "type": "string",
              "description": "The previous version of the body if the action was edited"
            }
          }
        }
      }
    },
    "reason": {
      "type": "string",
      "description": "The reason the pull request was removed from a merge queue if the action was dequeued"
    },
