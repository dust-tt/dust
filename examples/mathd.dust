```
// Extremely trivial seeded number generator.
mini_rand = (i, j) => {
  return 19*i + 23*j;
};

_fun = (env) => {
  let i = env["map"]["iteration"];
  let chosen = [];
  let data = env["state"]["RETRIEVAL"];
  for (let j = 0; j < data.length; j++) {
    let r = mini_rand(i, j) % data.length;
    chosen.push(data.splice(r, 1)[0]);
  }
  return chosen;
}
```
