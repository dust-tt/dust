{
  "name": "@dust-tt/client",
  "version": "1.2.6",
  "description": "Client for Dust API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dust-tt/dust.git"
  },
  "author": "Dust (Permutation Labs SAS)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dust-tt/dust/issues"
  },
  "homepage": "https://github.com/dust-tt/dust-sdk-js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/client.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:bundle",
    "build:types": "tsc",
    "build:bundle": "tsx esbuild.config.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "watch": "npm-watch",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/event-source-polyfill": "^1.0.5",
    "@types/node": "22.19.11",
    "@types/uuid": "^9.0.7",
    "esbuild": "^0.24.2",
    "npm-watch": "^0.11.0",
    "tslib": "^2.6.2",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@types/json-schema": "^7.0.15",
    "event-source-polyfill": "^1.0.31",
    "eventsource-parser": "^1.1.1",
    "zod": "^3.23.8"
  },
  "browser": {
    "tls": false,
    "net": false,
    "hot-shots": false
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "ignore": ""
    }
  }
}
