{
  "name": "@dust-tt/client",
  "version": "1.2.3",
  "description": "Client for Dust API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dust-tt/dust.git"
  },
  "author": "Dust (Permutation Labs SAS)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dust-tt/dust/issues"
  },
  "homepage": "https://github.com/dust-tt/dust-sdk-js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/client.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:bundle",
    "build:types": "tsc",
    "build:bundle": "tsx esbuild.config.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "watch": "npm-watch",
    "tsc": "tsc"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/event-source-polyfill": "^1.0.5",
    "npm-watch": "^0.11.0",
    "tslib": "^2.6.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "git://github.com:dust-tt/typescript-sdk.git#4a3d6b942e3982c818acd529de9b574c92776a2e",
    "@types/json-schema": "^7.0.15",
    "event-source-polyfill": "^1.0.31",
    "eventsource-parser": "^1.1.1",
    "zod": "^3.23.8"
  },
  "bundledDependencies": [
    "@modelcontextprotocol/sdk"
  ],
  "browser": {
    "tls": false,
    "net": false,
    "hot-shots": false
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "ignore": ""
    }
  }
}
