{
  "name": "@dust-tt/client",
  "version": "1.2.3",
  "description": "Client for Dust API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dust-tt/dust.git"
  },
  "author": "Dust (Permutation Labs SAS)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dust-tt/dust/issues"
  },
  "homepage": "https://github.com/dust-tt/dust-sdk-js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/client.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:bundle",
    "build:types": "tsc",
    "build:bundle": "tsx esbuild.config.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "watch": "npm-watch",
    "tsc": "tsc"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/event-source-polyfill": "^1.0.5",
    "@types/node": "^22.7.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "npm-watch": "^0.11.0",
    "prettier": "^3.0",
    "tslib": "^2.6.2",
    "tsx": "^4.19.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "git://github.com:dust-tt/typescript-sdk.git#628ebe48388549faae7e35504611af9ac2c6f5e4",
    "@types/json-schema": "^7.0.15",
    "event-source-polyfill": "^1.0.31",
    "eventsource-parser": "^1.1.1",
    "zod": "^3.23.8"
  },
  "bundledDependencies": [
    "@modelcontextprotocol/sdk"
  ],
  "browser": {
    "tls": false,
    "net": false,
    "hot-shots": false
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "ignore": ""
    }
  }
}
