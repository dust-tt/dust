    "crawlee": "^3.13.3-beta.10",
    "eslint-plugin-dust": "*",
    "express": "^4.18.2",
    "file-type": "^20.5.0",
    "googleapis-common": "^6.0.4",
    "gunzip-maybe": "^1.4.2",
