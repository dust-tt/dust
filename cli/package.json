    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "react": "^18.3.1",
    "update-notifier": "^7.3.1"
  },
  "engines": {
    "node": ">=21.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.16",
    "@types/react": "^19.0.12",
    "@types/update-notifier": "^6.0.8",
    "dotenv-flow": "^4.1.0",
