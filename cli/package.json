{
  "name": "@dust-tt/dust-cli",
  "version": "0.4.0",
  "description": "A command-line interface for interacting with Dust.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "dust": "dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "build:prod": "NODE_ENV=production tsup",
    "build:dev": "NODE_ENV=development tsup",
    "build": "NODE_ENV=development tsup",
    "dev": "NODE_ENV=development tsup --watch",
    "lint": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "start": "node dist/index.js"
  },
  "keywords": ["dust", "cli", "terminal", "command-line"],
  "author": "Dust",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dust-tt/dust",
    "directory": "cli"
  },
  "dependencies": {
    "@dust-tt/client": "^1.2.5",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "chalk": "^5.4.1",
    "clipboardy": "^4.0.0",
    "diff": "^8.0.2",
    "execa": "^9.6.0",
    "express": "^5.1.0",
    "glob": "^10.5.0",
    "ink": "^5.2.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "install": "^0.13.0",
    "jwt-decode": "^4.0.0",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "meow": "^13.2.0",
    "mime-types": "^3.0.1",
    "node-fetch": "^3.3.2",
    "npm": "^11.2.0",
    "open": "^10.1.0",
    "react": "^18.3.1",
    "update-notifier": "^7.3.1",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": "20.19.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.19.2",
    "@types/react": "^18.3.18",
    "@types/update-notifier": "^6.0.8",
    "diff": "^8.0.2",
    "dotenv-flow": "^4.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3"
  }
